{
  "name": "fridaycollective/laravel-gmail",
  "description": "Gmail API package for Laravel based on dacastro4/laravel-gmail",
  "keywords": [
    "api",
    "laravel",
    "Gmail"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alex & Dean",
      "email": "hey@fridaycollective.co.uk",
      "homepage": "https://fridaycollective.co.uk"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "google/apiclient": "^2.5",
    "illuminate/auth": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/config": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/database": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/routing": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/session": "~5.8|^6.0|^7.0|^8.0",
    "illuminate/support": "~5.8|^6.0|^7.0|^8.0",
    "swiftmailer/swiftmailer": "~5.8|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.0",
    "mockery/mockery": "^1.0",
    "squizlabs/php_codesniffer": "~3.4",
    "orchestra/testbench": "^4.0|^5.0|^6.0"
  },
  "autoload": {
    "psr-4": {
      "FridayCollective\\LaravelGmail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "FridayCollective\\LaravelGmail\\LaravelGmailServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
